<div class="{__NODE_ID__}" instance="{__INSTANCE__}">

    <div class="left buttons">
        <div class="nav button">
            <div class="icon fa fa-bars"></div>
        </div>
        <div class="notifications button">
            <div class="icon fa fa-bell"></div>
        </div>
    </div>

    <div class="center buttons">
        <div class="back button {BACK_BUTTON_HIDDEN_CLASS}">
            <div class="icon fa fa-level-up"></div>
        </div>
        <div class="close button {CLOSE_BUTTON_HIDDEN_CLASS}">
            <div class="icon fa fa-close"></div>
        </div>
    </div>

    <div class="right buttons">
        <div class="main button" some="{MAIN_BUTTON_SOME}">
            <div class="more_buttons" style="left: {MORE_BUTTONS_LEFT}px">
                {* some_buttons_gc *}
                <div class="ordering more_button">
                    <div class="icon fa fa-sort-alpha-asc ordering_by_name_indicator"></div>
                </div>
                <div class="tree more_button">
                    <div class="icon fa fa-code-fork"></div>
                </div>
                <div class="filter more_button">
                    <div class="icon fa fa-filter"></div>
                </div>
                <div class="search more_button">
                    <div class="icon fa fa-search"></div>
                </div>
                <div class="qrcode more_button">
                    <div class="icon fa fa-qrcode"></div>
                </div>
            </div>
        </div>

        <div class="more button">
            <div class="icon fa fa-ellipsis-v"></div>
        </div>
    </div>

</div>
