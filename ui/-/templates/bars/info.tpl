<div class="{__NODE_ID__}" instance="{__INSTANCE__}">

    <!-- cat -->
    <div class="cat">
        <div class="location">
            <div class="icon fa fa-code-fork"></div>
            <div class="name">{TREE_NAME}</div>
        </div>

        <div class="angle"></div>

        <!-- if cat/branch_node -->
        <div class="info_container">
            <div class="info">
                <!-- cat/branch_node -->
                <div class="node" cat_id="{ID}">
                    <div class="icon fa fa-chevron-right"></div>
                    <div class="name">{NAME}</div>
                </div>
                <!-- / -->
            </div>
        </div>
        <!-- / -->
    </div>
    <!-- / -->

    <!-- search -->
    <div class="search">
        <div class="location">
            <div class="icon fa fa-search"></div>
            <div class="name">поиск</div>
        </div>

        <div class="angle"></div>

        <div class="info_container">
            <div class="info">
                <div class="node">{TREES_NAMES}</div>
            </div>
        </div>
    </div>
    <!-- / -->

    <!-- if filter -->
    <div class="filters">
        <!-- filter -->
        <div class="filter division">
            <div class="icon fa fa-filter"></div>
            <div class="division">{DIVISION}</div>
            <div class="sep icon fa fa-arrow-right"></div>
            <div class="warehouse">{WAREHOUSE}</div>
        </div>
        <!-- / -->
    </div>
    <!-- / -->

</div>
