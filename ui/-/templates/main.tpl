<div class="{__NODE_ID__} {CLASS}" instance="{__INSTANCE__}">

    <div class="dock left" dock="left">
        {LEFT_DOCK_CONTENT}
    </div>

    <div class="center">
        <div class="bars">
            <div class="main bar">
                {MAIN_BAR}
            </div>

            <div class="more bar">
                {MORE_BAR}
            </div>

            <div class="some bar">
                {SOME_BAR}
            </div>

            <div class="info bar">
                {INFO_BAR}
            </div>
        </div>

        <div class="dock center" dock="center">
            {CENTER_DOCK_CONTENT}
            <div class="layer" layer="center">
                {CENTER_LAYER_CONTENT}
            </div>
        </div>

        <div class="layer" layer="fs">
            {FS_LAYER_CONTENT}
        </div>
    </div>

    <div class="dock right" dock="right">
        {RIGHT_DOCK_CONTENT}
    </div>

</div>
